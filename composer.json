{
  "name": "laraspace/laraspace-vue",
  "description": "The Laraspace admin template.",
  "keywords": [
    "laravel",
    "vue",
    "laraspace",
    "admin",
    "template"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "barryvdh/laravel-dompdf": "^0.8.5",
    "ckeditor/ckeditor": "^4.12",
    "ckfinder/ckfinder-laravel-package": "v3.5.1.2",
    "doctrine/dbal": "^2.9",
    "fideloper/proxy": "^4.0",
    "guzzlehttp/guzzle": "6.5",
    "laracasts/flash": "^2.0",
    "laravel/framework": "5.8.*",
    "laravel/tinker": "^1.0",
    "maatwebsite/excel": "^3.1",
    "predis/predis": "^1.1",
    "spatie/laravel-permission": "^2.37",
    "tintnaingwin/email-checker": "^2.0",
    "tymon/jwt-auth": "0.5.*",
    "unisharp/laravel-ckeditor": "^4.7",
    "unisharp/laravel-filemanager": "^2.0",
    "webklex/laravel-imap": "^1.4"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "nunomaduro/collision": "^2.0",
    "barryvdh/laravel-ide-helper": "^2.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Space/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
